<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Pressed -->
    <item
        android:state_pressed="true">

        <shape
            android:shape="rectangle">

            <solid android:color="@color/dark_green" />
        </shape>
    </item>

    <!-- Default -->
    <item>
        <shape
            android:shape="rectangle">

            <solid android:color="@color/green" />
        </shape>
    </item>

</selector>